<div>
  <h1>Free Agents</h1>
    <table class="table-auto w-full">
    <thead>
      <tr>
        <th class="text-left">Player</th>
        <th class="text-left">Position</th>
      </tr>
    </thead>
    <tbody>
      <% @players.each do |player| %>
        <tr>      
          <td class=""><%= player.name %></td>
          <td class=""><%= player.position%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
